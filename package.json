{
    "name": "firebase-edit",
    "version": "1.0.0",
    "description": "An Electron application with Vue and TypeScript",
    "main": "./out/main/index.js",
    "author": "example.com",
    "homepage": "https://electron-vite.org",
    "scripts": {
        "format": "prettier --write .",
        "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
        "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
        "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
        "typecheck": "yarn run typecheck:node && npm run typecheck:web",
        "start": "electron-vite preview",
        "dev": "tsc -w | electron-vite dev",
        "build": "yarn run format && yarn run lint && yarn run typecheck && electron-vite build",
        "postinstall": "electron-builder install-app-deps",
        "build:unpack": "yarn run build && electron-builder --dir",
        "build:win": "yarn run build && electron-builder --win",
        "build:mac": "yarn run build && electron-builder --mac",
        "build:linux": "yarn run build && electron-builder --linux"
    },
    "dependencies": {
        "@codemirror/autocomplete": "^6.16.2",
        "@codemirror/lang-json": "^6.0.1",
        "@codemirror/theme-one-dark": "^6.1.2",
        "@electron-toolkit/preload": "^3.0.0",
        "@electron-toolkit/utils": "^3.0.0",
        "@guolao/vue-monaco-editor": "^1.5.1",
        "autoprefixer": "^10.4.19",
        "codemirror": "^6.0.1",
        "firebase-admin": "^12.1.1",
        "fs-extra": "^11.2.0",
        "lodash": "^4.17.21",
        "postcss": "^8.4.38",
        "vue-codemirror": "^6.1.1",
        "vue-resizable": "^2.1.7",
        "vue-router": "^4.3.2",
        "vuex": "^4.1.0"
    },
    "devDependencies": {
        "@electron-toolkit/eslint-config": "^1.0.2",
        "@electron-toolkit/eslint-config-ts": "^1.0.1",
        "@electron-toolkit/tsconfig": "^1.0.1",
        "@rushstack/eslint-patch": "^1.7.1",
        "@types/node": "^18.19.9",
        "@vitejs/plugin-vue": "^5.0.3",
        "@vue/eslint-config-prettier": "^9.0.0",
        "@vue/eslint-config-typescript": "^12.0.0",
        "clsx": "^2.1.1",
        "electron": "^28.2.0",
        "electron-builder": "^24.9.1",
        "electron-vite": "^2.0.0",
        "eslint": "^8.56.0",
        "eslint-plugin-vue": "^9.20.1",
        "prettier": "^3.2.4",
        "tailwind-merge": "^2.3.0",
        "tailwindcss": "^3.4.3",
        "typescript": "^5.3.3",
        "vite": "^5.0.12",
        "vue": "^3.4.15",
        "vue-tsc": "^1.8.27"
    }
}
